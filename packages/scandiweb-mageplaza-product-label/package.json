{
    "name": "@scandiweb/mageplaza-product-label",
    "version": "1.0.1",
    "description": "Mageplaza Product Label frontend compatibility for ScandiPWA",
    "license": "UNLICENSED",
    "scandipwa": {
        "type": "extension"
    },
    "dependencies": {
        "eslint-plugin-header": "^3.1.1"
    },
    "eslintConfig": {
        "extends": "@scandipwa",
        "plugins": [
            "react",
            "header"
        ],
        "rules": {
            "max-lines": "off",
            "@scandipwa/scandipwa-guidelines/only-render-in-component": "off",
            "@scandipwa/scandipwa-guidelines/jsx-no-props-destruction": "off",
            "header/header": [
                2,
                "block",
                [
                    "*",
                    " * Mageplaza Product Label compatibility for ScandiPWA",
                    " * @copyright Scandiweb, Inc. All rights reserved.",
                    " "
                ],
                2
            ]
        }
    }
}
